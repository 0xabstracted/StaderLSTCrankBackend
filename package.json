{
  "name": "Stader-Liquid-Staking-Crank",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run build && npm run typeorm migration:run -- -d ./src/infrastructure/typeorm/typeorm.config.ts",
    "typeorm:generate-migration": "npm run build && npm run typeorm -- -d ./src/infrastructure/typeorm/typeorm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/infrastructure/typeorm/typeorm.config.ts migration:revert",
    "typeorm:run-migrations-prod": "NODE_ENV=production npm run typeorm migration:run -- -d ./src/infrastructure/typeorm/typeorm.config.ts",
    "typeorm:generate-migration-prod": "NODE_ENV=production npm run build && npm run typeorm -- -d ./src/infrastructure/typeorm/typeorm.config.ts migration:generate ./prod-migrations/$npm_config_name",
    "typeorm:create-migration-prod": "NODE_ENV=production npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration-prod": "NODE_ENV=production npm run typeorm -- -d ./src/infrastructure/typeorm/typeorm.config.ts migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.691.0",
    "@coral-xyz/anchor": "^0.29.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.2.2",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/typeorm": "^10.0.2",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.95.4",
    "axios": "^1.7.9",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security",
    "ioredis": "^5.4.1",
    "mongodb": "^5.9.2",
    "mongoose": "^8.9.2",
    "pg": "^8.13.1",
    "pm2": "^5.4.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
